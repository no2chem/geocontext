{
  "name": "geocontext",
  "version": "0.1.1",
  "description": "Cross platform geographical context API",
  "main": "build/src/node.js",
  "browser": {
    "./build/src/node.js": "build/src/browser.js"
  },
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts --timeout 20000",
    "check": "gts check",
    "clean": "gts clean",
    "prepublish": "tsc -p .;rollup -c > build/src/node.js && BROWSER=true rollup -c > build/src/browser.js",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "typedoc": "typedoc --out docs $(pwd)/src --target es6 --mode file --tsconfig ./tsconfig.json --excludePrivate --excludeProtected --exclude **/*.spec.ts",
    "posttest": "npm run check"
  },
  "keywords": [
    "geolocation",
    "geocontext",
    "cross-platform",
    "macos",
    "location"
  ],
  "author": {
    "name": "Michael Wei"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "gts": "^0.5.3",
    "mocha": "^5.0.1",
    "pre-commit": "^1.2.2",
    "rollup": "^0.56.3",
    "rollup-plugin-replace": "^2.0.0",
    "ts-node": "^5.0.0",
    "typedoc": "^0.10.0",
    "typescript": "~2.6.1"
  },
  "pre-commit": [
    "check",
    "test"
  ],
  "optionalDependencies": {
    "macos-location": "^1.2.0"
  },
  "dependencies": {
    "@types/node": "^9.4.6",
    "iplocation": "^5.0.0"
  }
}
